<script src="~/Content/Scripts/jquery-1.8.3.min.js"></script>
<script src="~/Content/Scripts/bootstrap.min.js"></script>
<script src="~/Content/Scripts/jquery.bxslider.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {

        $('.slider').bxSlider({
            auto: true,
            controls: true
        });
        $('.newsSlider').bxSlider({
            auto: true,
            slideWidth: 295,
            minSlides: 3,
            maxSlides: 3,
            slideMargin: 15,
            pause: 8000,
            speed: 1500,
            controls: false
        });
        $('.referenceSlider').bxSlider({
            auto: true,
            slideWidth: 295,
            minSlides: 5,
            maxSlides: 5,
            slideMargin: 15,
            pause: 8000,
            speed: 1500,
            controls: false
        });
        $('.tab a').click(function (e) {
            e.preventDefault()
            $(this).tab('show')
        })
    });
</script>
</head>

